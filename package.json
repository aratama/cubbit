{
  "name": "cubbit",
  "version": "0.0.1",
  "main": "",
  "scripts": {
    "pretest": "psc bower_components/purescript-*/src/**/*.purs src/**/*.purs",
    "test": "psc-bundle --module Game.Cubbot.Test --main Game.Cubbot.Test --output public/index.js output/**/*.js",
    "posttest": "node public/index.js",
    "prebuild": "psc bower_components/purescript-*/src/**/*.purs src/**/*.purs",
    "build": "psc-bundle --module Main --main Main output/**/*.js | browserify - > public/index.js",
    "electron": "electron-packager public cubbit --platform=\"win32\" --arch=x64 --version=1.4.10 --overwrite --icon=./public/icon.ico",
    "server": "start http-server public",
    "publish": "cpx public/**/*.* docs",
    "clean": "rimraf output node_modules bower_components public/index.js cubbit-win32-x64 docs",
    "license": "node res/license.js"
  },
  "author": "aratama",
  "license": "UNLICENSED",
  "directories": {},
  "description": "",
  "dependencies": {
    "glob": "^7.1.1",
    "virtual-dom": "^2.1.1"
  }
}
